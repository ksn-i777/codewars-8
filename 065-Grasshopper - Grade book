function getGrade (s1, s2, s3) {
  // Code here
  let middle = (s1+s2+s3)/3
  if (middle >=0 && middle <60) {
    return 'F'
  } else if (middle >=60 && middle <70) {
    return 'D'
  } else if (middle >=70 && middle <80) {
    return 'C'
  } else if (middle >=80 && middle <90) {
    return 'B'
  } else if (middle >=90) {
    return 'A'
  }
}
