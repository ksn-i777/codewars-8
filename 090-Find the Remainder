function remainder(n, m){
  if (n > m) {
    let answer = n % m;
    if (m === 0) {
      return NaN;
    }
    else {
      return answer;
    }
  }
  else if (m > n) {
    let answer = m % n;
    if (n === 0) {
      return NaN;
    }
    else {
      return answer;
    }
  }
  else {
    let answer = n % m;
    return answer;
  }
}
